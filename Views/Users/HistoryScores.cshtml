@{
    var scores = ViewBag.HistoryScores;
}
@{
    ViewBag.Title = "历史成绩";
}

<h1>历史成绩</h1>

<table class="table">
    <thead>
        <tr>
            <th>试卷名称</th>
            <th>课程名称</th>
            <th>成绩</th>
            <th>考试时间</th>
            <th>操作</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var exam in Model)
        {
            <tr>
                <td>@exam.PaperName</td>
                <td>@exam.CourseName</td>
                <td>@exam.Score</td>
                <td>@exam.BegainTime</td>
                <td>
                    <a href="@Url.Action("ResultPage", "Users", new { studentExamId = exam.StudentExamId })" class="btn btn-primary">查看详情</a>
                </td>
            </tr>
        }
    </tbody>
</table>